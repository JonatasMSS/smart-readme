{
  "name": "smart-readme",
  "displayName": "Smart Readme",
  "description": "An inteligent Readme generator that can use or not Generative AI to create awesomes readmes!",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "smart-readme.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "extension.setApiToken",
        "title": "Set Api Token",
        "category": "API"
      },
      {
        "command": "extension.getApiToken",
        "title": "Show API",
        "category": "API"
      },
      {
        "command": "extension.resetApiToken",
        "title": "Reset Api Token",
        "category": "API"
      },
      {
        "command": "extension.generateReadme",
        "title": "Generate Readme",
        "category": "API"
      },
      {
        "command": "extension.getCurrentDir",
        "title": "Get Current Dir",
        "category": "UTIL"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.90.0",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@google/generative-ai": "^0.12.0"
  }
}
